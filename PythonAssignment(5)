strg = input("Enter a string")
print("Reversed string: ", strg[::-1])

def checkpalindrome():
 if (strg == strg[::-1]):
     print("The string is a palindrome")
 else:
     print("The string is not a palindrome")

def insertSpace():
    strn=strg
    print(" ".join(strg))

checkpalindrome()
insertSpace()
